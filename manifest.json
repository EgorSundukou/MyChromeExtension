{
  "manifest_version": 3,
  "name": "AutoDeclineFB",
  "version": "1.0",
  "description": "Автоклик по кнопке 'Отклонить' с управлением через popup",
  "permissions": ["activeTab", "scripting","alarms"],
  "action": {
    "default_popup": "popup.html"

  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
